<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - LayoutTry</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/LayoutTry.styles.css" asp-append-version="true" />

    <!-- Favicons -->
    @* <link href="assets/img/favicon.png" rel="icon">
    <link href="assets/img/apple-touch-icon.png" rel="apple-touch-icon"> *@

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i"
          rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/vendor/aos/aos.css" rel="stylesheet" asp-append-version="true">
    <link href="~/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet" asp-append-version="true">
    <link href="~/vendor/boxicons/css/boxicons.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/vendor/glightbox/css/glightbox.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/vendor/swiper/swiper-bundle.min.css" rel="stylesheet" asp-append-version="true">

    <!-- Template Main CSS File -->
    <link href="~/css/style.css" rel="stylesheet" asp-append-version="true">

</head>
<body>
    <header>
        <nav class="navbar nav-menu navbar-expand-sm navbar-toggleable-sm border-bottom box-shadow mb-3">
            <div class="container-fluid">
            </div>
        </nav>
    </header>
    <!-- ======= Mobile nav toggle button ======= -->
    @* <i class="bi bi-list mobile-nav-toggle d-xl-none"></i> *@
    <!-- ======= Header ======= -->
    <header id="header">
        <div class="d-flex flex-column">

            <div class="profile">
                @* <img src="assets/img/profile-img.jpg" alt="" class="img-fluid rounded-circle"> *@
                <h1 class="mt-5 text-light"><a href="#">後台頁面</a></h1>
                
            </div>

            <nav id="navbar" class="nav-menu navbar">
                <ul>
                    <li>
                        <a href="#hero" class="nav-link scrollto active">
                            <i class="bx bx-home"></i>
                            <span>Home</span>
                        </a>
                    </li>
                    <li>
                         @* <a class="nav-link scrollto" asp-area="" asp-controller="Member" asp-action="List">
    <i class="bx bx-user"></i>
    <span>會員管理</span> *@
                        <a href="#about" class="nav-link scrollto">
                            <i class="bx bx-user"></i> 
                            <span>會員管理</span></a>
                    </li>
                    <li>
                        <a href="#resume" class="nav-link scrollto">
                            <i class="bx bxs-coupon"></i>
                            <span>售票系統管理</span>
                        </a>
                    </li>
                    <li>
                        <a href="#portfolio" class="nav-link scrollto">
                            <i class="bx bx-music"></i>
                            <span>演出管理</span>
                        </a>
                    </li>

                    <li>
                        <a href="#contact" class="nav-link scrollto">
                            <i class="bx bx-arch"></i>
                            <span>場館管理</span>
                        </a>
                    </li>
                    <li>
                        <a href="#contact" class="nav-link scrollto">
                            <i class='bx bxs-hand'></i>
                            <span>二手票券交易平台管理</span>
                        </a>
                    </li>
                </ul>
            </nav><!-- .nav-menu -->
        </div>
    </header><!-- End Header -->
    <main id="main">
    <section >
        <div class="container">
            @RenderBody()
            @* <main role="main" class="pb-3">
               
            </main> *@
        </div>
    </section>
    </main>
    <a href="#" class="back-to-top d-flex align-items-center justify-content-center">
        <i class="bi bi-arrow-up-short"></i>
    </a>
    @* <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - prjMvcCoreDemo - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer> *@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
